{
  "name": "uddaan-consultancy",
  "version": "1.0.0",
  "description": "Uddaan Consultancy - Your Gateway to Global Opportunities",
  "main": "index.js",
  "scripts": {
    "dev": "bash scripts/dev.sh",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm run dev",
    "db:up": "docker compose -f backend/docker-compose.yml up -d db",
    "prisma:generate": "cd backend && npm run prisma:generate",
    "prisma:migrate": "cd backend && npm run prisma:migrate",
    "seed": "cd backend && npm run seed",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "setup": "./start.sh",
    "health": "curl -f http://localhost:5000/health || echo 'Backend not running'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "education",
    "consulting",
    "jobs",
    "international"
  ],
  "author": "Uddaan Consultancy",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "mongoose": "^8.17.1",
    "nodemailer": "^7.0.5",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.3",
    "speakeasy": "^2.0.0"
  }
}